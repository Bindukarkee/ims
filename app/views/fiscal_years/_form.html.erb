 <div class="container">
      <div class="row">
        <div class="col-md-offset-2 col-md-6">
          <div class="well well1">

            <%= form_for @fiscal_year do |f| %>
    <form>

        <div class="form-group">
          <div class="row">
            <div class="col-sm-3">
              <label>Name</label></div>
            <div class="col-md-8">
              <%= f.text_field :name, {class: "form-control"} %>
            </div>
          </div>
        </div>

        <div class="form-group">
          <div class="row">
            <div class="col-sm-3">
              <label>Start Date </label></div>
            <div class="col-md-8">
              <%= f.text_field :start_date, :class => "form-control ", "data-provide" => 'datepicker', :id =>"datepicker", :placeholder => "YYYY/MM/DD" %>
            </div>
          </div>
        </div>

      <div class="form-group">
        <div class="row">
          <div class="col-sm-3">
            <label>End Date</label></div>
          <div class="col-md-8">
            <%= f.text_field :end_date, :class => "form-control ", :id =>"datepicker1", :placeholder => "YYYY/MM/DD" %>
          </div>
        </div>
      </div>

      

       <p> <button type="submit" class="btn btn-primary">SAVE</button>
        </p>


    </form>
<% end %>
            </div>
          </div>
        </div>
      </div>

 <script>
   $('#datepicker').datepicker({
     format: "yyyy/mm/dd",
   }).on('changeDate', function(e){
     $(this).datepicker('hide');
   });
 </script>

 <script>
   $('#datepicker1').datepicker({
     format: "yyyy/mm/dd",
   }).on('changeDate', function(e){
     $(this).datepicker('hide');
   });
 </script>

